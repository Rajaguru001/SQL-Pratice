create database Attendence;
use Attendence;
create table Attendencesheet(
 ID int primary key,
 name varchar(30) not null,
 rollno int,
 noofclassheld int not null,
 noofPresent int
 
 );
 insert into Attendencesheet(id,name,rollno,noofClassheld,noofPresent)values(123,"Dhoni",07,100,77);
 insert into Attendencesheet(id,name,rollno,noofClassheld,noofPresent)values(124,"Raina",03,100,80);
  insert into Attendencesheet(id,name,rollno,noofClassheld,noofPresent)values(125,"jadeja",08,100,85);
